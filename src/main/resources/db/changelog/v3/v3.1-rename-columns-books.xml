        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="3.1" author="Aleksandr">
        <renameColumn
                schemaName="model"
                tableName="books"
                oldColumnName="pageCount"
                newColumnName="page_count"
                columnDataType="int"/>
        <renameColumn
                schemaName="model"
                tableName="books"
                oldColumnName="publicationDate"
                newColumnName="publication_date"/>
    </changeSet>

</databaseChangeLog>